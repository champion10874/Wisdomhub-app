{
  "name": "wisdomhub-notifications",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "cross-env pm2 start ./build/src/app.js -i 5 --attach --watch | pino-pretty -c",
    "stop": "cross-env pm2 stop all",
    "delete": "cross-env pm2 delete all",
    "dev": "cross-env nodemon -r tsconfig-paths/register src/app.ts | pino-pretty -c",
    "lint:check": "cross-env eslint 'src/**/*.ts'",
    "lint:fix": "cross-env eslint 'src/**/*.ts' --fix",
    "prettier:check": "cross-env prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "cross-env prettier --write 'src/**/*.{ts,json}'",
    "build": "cross-env tsc --project tsconfig.json && tsc-alias -p tsconfig.json && ts-node tools/copyAssets.ts",
    "test": "cross-env jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false"
  },
  "keywords": [],
  "author": "Or Hasson",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/amqplib": "^0.10.5",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "minimatch": "^9.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.12.2",
    "@hassonor/wisdomhub-shared": "^0.1.14",
    "amqplib": "^0.10.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "email-templates": "^11.1.1",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^11.2.0",
    "http-status-codes": "^2.3.0",
    "nodemailer": "^6.9.12",
    "pino-pretty": "^10.3.1",
    "typescript": "^5.4.2",
    "typescript-transform-paths": "^3.4.7",
    "winston": "^3.12.0"
  }
}
